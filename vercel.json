{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "build": "npm run build",
        "output": "chaos-wealth-navigator/dist"
      }
    },
    {
      "src": "api/api.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api(.*)",
      "dest": "/chaos-wealth-navigator/api/api.py",
      "methods": ["GET", "POST"]
    },
    {
      "src": "/(.*)",
      "dest": "/chaos-wealth-navigator/dist/index.html"
    }
  ]
}